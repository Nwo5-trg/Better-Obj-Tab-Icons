			"type": "title",
			"name": "Monster Tab"
		},
		"monstertab-toggle": 
		{
			"type": "bool",
			"name": "Enable Custom 'monster-tab' Icons",
			"default": true
		},
		"pixeltab-title":
		{
			"type": "title",
			"name": "Pixel Tab"
		},
		"pixeltab-toggle": 
		{
			"type": "bool",
			"name": "Enable Custom 'pixel-tab' Icons",
			"default": true
