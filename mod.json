{
	"geode": "4.1.2",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074"
	},
	"id": "nwo5.better_object_tab_icons",
	"name": "Better Object Tab Icons",
	"version": "v1.1.2",
	"developer": "Nwo5",
	"description": "Better and Customisable Object Tab Icons!",
	"dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.11.0",
            "importance": "required"
        }
    ],
	"resources": {
        "sprites": [
            "resources/*.png"
        ]
    },
	"tags":[
		"editor",
		"enhancement",
		"customization"
	],
	"links": {
		"source": "https://github.com/nwo5-trg/Better-Obj-Tab-Icons"
	},
	"settings": {
		"blocktab-title":
		{
			"type": "title",
			"name": "Block Tab (No built-in texture, only for texture packs.)"
		},
		"blocktab-toggle": 
		{
			"type": "bool",
			"name": "Enable Custom 'block-tab' Icons",
			"default": false
		},
		"outlinetab-title":
		{
			"type": "title",
			"name": "Outline Tab (No built-in texture, only for texture packs.)"
		},
		"outlinetab-toggle": 
		{
			"type": "bool",
			"name": "Enable Custom 'outline-tab' Icons",
			"default": false
		},
		"hazardtab-title":
		{
			"type": "title",
			"name": "Hazard Tab (No built-in texture, only for texture packs.)"
		},
		"hazardtab-toggle": 
		{
			"type": "bool",
			"name": "Enable Custom 'hazard-tab' Icons",
			"default": false
		},
		"threedtab-title":
		{
			"type": "title",
			"name": "3D Tab"
		},
		"threedtab-toggle": 
		{
			"type": "bool",
			"name": "Enable Custom '3d-tab' Icons",
			"default": true
		},
		"threedtab-alt":
		{
			"type": "bool",
			"name": "Enable Alternate Texture For '3d-tab'",
			"enable-if": "threedtab-toggle",
			"default": false
		},
		"portaltab-title":
